{
	"name": "someone/wp-cli-batch-process",
	"description": "WordPress plugin to batch processes results from CSV for WP_Query",
	"type": "wordpress-plugin",
	"license": "GPLv2 or later",
	"authors": [
		{
			"name": "Someone",
			"email": "pete@petenelson.com"
		}
	],
	"require": {
		"php": ">=7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7",
		"10up/phpcs-composer": "dev-master"
	},
	"autoload": {
		"psr-4": {
		  "WpCliBatchProcess\\": "includes/classes/"
		}
	},
	"scripts": {
		"test":"./vendor/bin/phpunit",
		"lint": ["./vendor/bin/phpcs ./includes", "./tests/unit"],
		"fix":["./vendor/bin/phpcbf ./includes", "./vendor/bin/phpcbf ./tests/unit"],
		"test:wordpress": "phpunit --config=phpunit-integration.xml"
	}
}