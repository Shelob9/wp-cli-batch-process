{
	"name": "someone/plugin-name",
	"description": "WordPress plugin with WP CLI commands to do soemthing",
	"type": "wordpress-plugin",
	"license": "GPLv2 or later",
	"authors": [
		{
			"name": "Someone",
			"email": "pete@petenelson.com"
		}
	],
	"require": {
		"php": ">=7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7",
		"10up/phpcs-composer": "dev-master"
	},
	"autoload": {
		"psr-4": {
		  "PluginNamespace\\": "includes/classes/"
		}
	},
	"scripts": {
		"test":"./vendor/bin/phpunit",
		"lint": "./vendor/bin/phpcs ./includes ./tests/unit",
		"fix":"./vendor/bin/phpcbf ./includes ./tests/unit"
	}
}